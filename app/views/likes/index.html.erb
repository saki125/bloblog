<header class='top-header'>
  <div class='nav'>
      <ul class='lists-left'>
      <%= link_to image_tag("test_icon.png", class: "top-icon"), root_path %>
      </ul>
      <ul class='lists-center'>
        <div class="mypage">My Page</div>
      </ul>
      <ul class='lists-right'>
        <li><%= link_to "マイページに戻る", user_path(current_user.id), class: "remypage" %></li>
      </ul>
  </div>
</header>
<body>
  <p class = "blolist blo-like">いいねした投稿一覧</p>
  <% @like_list.reverse_each do |post| %>
  <%= link_to post_path(post.id) do %>
    <div class="postlist">
      <p><%= image_tag post.image,class: 'imgtag' %></p>
      <p class="post-titles"><%= post.title %></p>
    </div>
  <% end %>
  <% end %>

</body>

